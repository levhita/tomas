meta {
  name: Get User
  type: http
  seq: 2
}

get {
  url: {{server_url}}/users/:id
  body: none
  auth: inherit
}

params:path {
  id: {{userId}}
}

script:pre-request {
  const userId = await bru.prompt('userId', 'userId', bru.getVar('userId'))
  bru.setVar("userId", userId);
}

script:post-response {
  bru.setVar("userId", res.body.id);
}
