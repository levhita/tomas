{
  "name": "tomas-api",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "test": "dotenv -e .env.test -- jest",
    "test:watch": "dotenv -e .env.test -- jest --watch",
    "test:coverage": "dotenv -e .env.test -- jest --coverage --forceExit",
    "test:integration": "dotenv -e .env.test -- jest tests/integration",
    "test:unit": "dotenv -e .env.test -- jest --config jest.unit.config.js",
    "test:validate": "dotenv -e .env.test -- node tests/validate.js",
    "test:errors": "dotenv -e .env.test -- jest -f",
    "build-docs": "swagger-jsdoc -d swagger.config.js ./src/routes/*.js ./src/middleware/*.js -o ./docs/swagger.json"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "~1.9.1",
    "mysql2": "^3.12.0",
    "pinia": "^2.3.0"
  },
  "devDependencies": {
    "dotenv-cli": "^8.0.0",
    "jest": "^30.0.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.1.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  }
}
